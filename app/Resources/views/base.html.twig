<!DOCTYPE html>
{% trans_default_domain 'SonataAdminBundle' %}
<html>
    <head>
        <meta charset="UTF-8" />
        <title>
            {% block title %}
                {{ 'title' | trans() }}
            {% endblock %}
        </title>
        {% block stylesheets %}
            <link type="text/css" rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">
            <link type="text/css" rel="stylesheet" href="{{ asset('bundles/omerteam/css/new_team.css') }}">
            <link type="text/css" rel="stylesheet" href="{{ asset('bundles/omerteam/css/animate.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block javascripts %}
            <script type = "text/javascript" src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
            <script type = "text/javascript" src="{{ asset('assets/vendor/symfony-collection/jquery.collection.js') }}"></script>
            <script type = "text/javascript" src="{{ asset('bundles/omerteam/js/wow.min.js') }}"></script>
            <script>new WOW().init();</script>
        {% endblock %}
        {% block body %}

            {% block header %}
                {% include ':layout:_navbar.html.twig' %}
            {% endblock %}

            {% block content %}
            {% endblock %}

            {% block footer %}
            {% endblock %}

        {% endblock %}
    </body>
</html>
