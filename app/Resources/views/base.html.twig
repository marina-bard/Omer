<!DOCTYPE html>
{% trans_default_domain 'SonataAdminBundle' %}
<html>
    <head>
        <meta charset="UTF-8" />
        <title>
            {% block title %}
                {{ 'title' | trans() }}
            {% endblock %}
        </title>
        {% block stylesheets %}
            <link type="text/css" rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">
            <link type="text/css" rel="stylesheet" href="{{ asset('bundles/omerteam/css/new_team.css') }}">
            <link type="text/css" rel="stylesheet" href="{{ asset('bundles/omerteam/css/animate.css') }}">
            <link type="text/css" rel="stylesheet" href="{{ asset('bundles/sonatacore/vendor/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css') }}">
            <link type="text/css" rel="stylesheet" href="{{ asset('bundles/omerteam/css/font-awesome/css/font-awesome.min.css') }}">
        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('bundles/omerteam/images/favicon/favicon.ico') }}" type="image/x-icon" />
        <link rel="apple-touch-icon" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon.png') }}" />
        <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon-57x57.png') }}" />
        <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon-72x72.png') }}" />
        <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon-76x76.png') }}" />
        <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon-114x114.png') }}" />
        <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon-120x120.png') }}" />
        <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon-144x144.png') }}" />
        <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon-152x152.png') }}" />
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('bundles/omerteam/images/favicon/apple-touch-icon-180x180.png') }}" />

    </head>
    <body>
        {% block javascripts %}
            <script type = "text/javascript" src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
            <script type = "text/javascript" src="{{ asset('bundles/omerteam/js/jquery.easing.min.js') }}"></script>
            <script type = "text/javascript" src="{{ asset('assets/vendor/symfony-collection/jquery.collection.js') }}"></script>
            <script type = "text/javascript" src="{{ asset('bundles/omerteam/js/wow.min.js') }}"></script>
            <script type = "text/javascript" src="{{ asset('bundles/omerteam/js/moment-with-locales.min.js') }}"></script>
            <script type = "text/javascript" src="{{ asset('bundles/sonatacore/vendor/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js') }}"></script>
            <script type = "text/javascript" src="{{ asset('bundles/omerteam/js/main.js') }}"></script>
            <script>new WOW().init();</script>
        {% endblock %}
        {% block body %}

            {% block header %}
                {% include ':layout:_navbar.html.twig' %}
            {% endblock %}

            {% block content %}
            {% endblock %}

            {% block footer %}
                {% include 'OmerTeamBundle:Default:_footer.html.twig' %}
            {% endblock %}

        {% endblock %}
    </body>
</html>
